<%= decidim_form_for([current_voting, @form], url: { action: "update" }, html: { class: "form edit_polling_station" })  do |f| %>
  <%= render partial: "form", object: f, locals: { title: t(".title") } %>

  <div class="button--double form-general-submit">
    <%= f.submit t(".update"), class: "button button__sm md:button__lg button__secondary" %>
  </div>
<% end %>
